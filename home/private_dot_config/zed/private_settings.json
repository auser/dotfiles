{
  "theme": "ayu-dark",
  "vim_mode": true,
  "tab_size": 2,

  "project_panel": {
    "dock": "left"
  },

  "terminal": {
    "shell": {
      "with_arguments": {
        "program": "tmux",
        "args": ["new", "-As", "zed"]
      }
    },
    "working_directory": "current_project_directory",
    "toolbar": {
      "breadcrumbs": true
    }
  }
}
